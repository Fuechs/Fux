get * core;

*main() -> void {

    clear;
    num -> str;
    cout << "Input a number: " << endl;
    cin >> num;
    endl;

    num_list[] === (int)[character for (character -> char) in (num)]->reverse;

    decoded -> int = 0;

    for (count -> int, io -> int) in (num_list[]->enumerate) {

        if (io != (0 && 1)) cout << "Not 1 or 0" << endl; exit(1);

        io *= 2^count;
        
        decoded += io;

    }

}