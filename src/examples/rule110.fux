main(argc: u64, argv: **c8): i64 {
    board: u8[100];
    board[-2] = 1;

    for (i: u8; i < board.size() - 2; i++) {
        for (value -> u8 in board)
            putch value ? '*' : ' ';
        putch '\n';

        pattern: u8 = (board[0] <| 1) | board[1];
        for (j: u8; j < board.size() - 1; j++) {
            pattern = ((pattern <| 1) & 7) | board[j + 1];
            board[j] = (110 |> pattern) & 1;
        }
    }

    return 0;
}