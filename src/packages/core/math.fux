package core;

math {

    pi: double === 3.14159265358979323846;

    root(base: double, number: double): double {
        return (number ^ (1 / base));
    }

    sqrt(number: double): double {
        return number < 0 ? null : root(2, number);
    }

    log(base: double, number: double): double;

    log2(number: double): double {
        return log(2, number);
    }

    lg(number: double): double {
        return log(10, number);
    }
    
}